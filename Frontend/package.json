{
  "name": "Frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "lint": "eslint .",
    "start": "react-native start",
    "build:ios": "react-native run-ios --mode=Debug",
    "//": "JEST",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "//": "DETOX",
    "detox:clear:build": "detox rebuild-framework-cache",
    "//": "<<<< IOS DETOX>>>> ",
    "detox:build:ios:debug": "detox build --configuration ios.sim.debug",
    "detox:test:ios:debug": "detox test --configuration ios.sim.debug",
    "detox:test:ios:debug:record": "detox test --configuration 'ios.sim.debug' --take-screenshots all --record-videos all",
    "detox:build:ios:release": "detox build --configuration ios.sim.release",
    "detox:test:ios:release": "detox test --configuration ios.sim.release",
    "detox:test:ios:pipeline": "detox test --configuration ios.sim.release.pipeline --take-screenshots all --record-videos all --loglevel verbose",
    "//": "<<<< ANDROID DETOX >>>>",
    "detox:build:android:debug": "detox build --configuration android.emu.debug",
    "detox:test:android:debug": "detox test --configuration android.emu.debug",
    "detox:test:android:debug:record": "detox test --configuration 'android.emu.debug' --take-screenshots all --record-videos all",
    "detox:build:android:release": "detox build --configuration android.emu.release.pipeline",
    "detox:test:android:pipeline": "detox test --configuration android.emu.release.pipeline --take-screenshots all --record-videos all --loglevel verbose"
  },
  "dependencies": {
    "@hookform/devtools": "^4.3.1",
    "@hookform/resolvers": "^3.3.1",
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "axios": "^1.4.0",
    "jwt-decode": "^3.1.2",
    "react": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-native": "0.72.4",
    "react-native-config": "^1.5.1",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.2.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "detox": "^20.11.4",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
